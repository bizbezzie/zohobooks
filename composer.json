{
    "name": "bizbezzie/zohobooks",
    "description": "This Package will help to call ZohoBooks API",
    "license": "MIT",
    "authors": [
        {
            "name": "Navin Patil",
            "email": "winavin@bizbezzie.com",
            "homepage": "http://www.bizbezzie.com/authors/winavin"
        }
    ],
    "homepage": "https://github.com/bizbezzie/zohobooks",
    "keywords": ["Laravel", "Zohobooks"],
    "require": {
        "illuminate/support": "~9",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/socialite": "^5.5",
        "socialiteproviders/zoho": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~9"
    },
    "autoload": {
        "psr-4": {
            "Bizbezzie\\Zohobooks\\": "src/",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bizbezzie\\Zohobooks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bizbezzie\\Zohobooks\\ZohobooksServiceProvider"
            ],
            "aliases": {
                "Zohobooks": "Bizbezzie\\Zohobooks\\Facades\\Zohobooks",
                "ZohobooksHelper": "Bizbezzie\\Zohobooks\\Facades\\ZohobooksHelper",
                "Contact": "Bizbezzie\\Zohobooks\\Facades\\Contact",
                "Invoice": "Bizbezzie\\Zohobooks\\Facades\\Invoice"
            }
        }
    }
}
